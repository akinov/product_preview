<h1>ユーザー一覧</h1>

<% if @users.present? %>
<ul class="users">
  <% @users.each do |user| %>
    <li>
		<%= link_to user do %>
      <%= user.id %>
      <%= user.userinfo.nickname %>
      <%= user.userinfo.sex %>
      <%= user.userinfo.age %>
      <%= user.userinfo.live %>
      <%= user.userinfo.country %>
		<% end %>
    </li><br>
  <% end %>
</ul>
<% end %>